<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_start"
    app:startDestination="@id/auth"
    >

    <fragment
        android:id="@+id/auth"
        android:name="com.petapp.capybara.auth.AuthFragment"
        android:label="auth"
        tools:layout="@layout/fragment_auth"
        >
        <action
            android:id="@+id/to_main"
            app:destination="@id/nav_graph_main"
            app:launchSingleTop="true"
            app:popUpTo="@id/nav_graph_start"
            app:popUpToInclusive="true"
            />
    </fragment>

    <include app:graph="@navigation/nav_graph_main" />

</navigation>
